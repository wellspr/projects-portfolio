{
  "name": "ssr-react-app",
  "version": "1.0.0",
  "description": "",
  "main": "build/js/server.js",
  "scripts": {
    "start": "node build/js/server.js",
    "local": "node -r dotenv/config build/js/server.js dotenv_config_path=.env",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node -r dotenv/config build/js/server.js dotenv_config_path=.env\"",
    "dev:build-server": "webpack --config webpack.server.dev.js --watch",
    "dev:build-client": "webpack --config webpack.client.dev.js --watch",
    "prod:build-server": "webpack --config webpack.server.js",
    "prod:build-client": "webpack --config webpack.client.js",
    "prod:copy-files": "(node copy_files.js) && (cp package-lock.json prod) && (cp .env prod)",
    "prod:build": "npm run prod:build-server && npm run prod:build-client && npm run prod:copy-files",
    "prod": "node -r dotenv/config prod/build/js/server.js dotenv_config_path=.env",
    "build": "(npm run clean:dev) && webpack --config webpack.server.dev.js && webpack --config webpack.client.dev.js",
    "clean:dev": "(rm -rf public/dist) && (rm -rf build)",
    "clean:prod": "(rm -rf prod)"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "axios": "^0.24.0",
    "babel-loader": "^8.2.3",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.2",
    "compression": "^1.7.4",
    "concurrently": "^6.5.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "css-loader": "^6.5.1",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.3",
    "file-loader": "^6.2.0",
    "helmet": "^5.0.1",
    "html-loader": "^3.1.0",
    "mini-css-extract-plugin": "^2.5.3",
    "npm-run-all": "^4.1.5",
    "react-bootstrap": "^2.1.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.3.0",
    "redux-thunk": "^2.4.1",
    "serialize-javascript": "^6.0.0",
    "style-loader": "^3.3.1",
    "transform-class-properties": "^1.0.0-beta",
    "url-loader": "^4.1.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.15"
  },
  "engines": {
    "node": "16.x.x"
  }
}
